{
  "name": "react-apollo-feature-flag",
  "version": "0.1.1",
  "description": "GraphQL Feature Flag implementation for React under Apollo Client",
  "author": "peaonunes",
  "homepage": "https://github.com/peaonunes/react-apollo-feature-flag",
  "repository": {
    "type": "git",
    "url": "git@github.com:peaonunes/react-apollo-feature-flag.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production"
  },
  "keywords": [
    "graphql",
    "react",
    "react-apollo",
    "apollo client",
    "feature flag",
    "feature toggle"
  ],
  "license": "BSD-3-Clause",
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "jsxBracketSameLine": true
  },
  "peerDependencies": {
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "prop-types": "^15.6.2",
    "react": "^15.3.0 || ^16.2.0",
    "react-apollo": "^2.1.11",
    "react-dom": "^15.3.0 || ^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "apollo-boost": "^0.1.15",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.3.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
